# DataGovIN Sentinel Actor - Environment Variables Example
# Copy this file to .env and configure your settings

# API Authentication (optional for public data)
DATAGOVIN_API_KEY=your_api_key_here
DATAGOVIN_OAUTH_TOKEN=your_oauth_token_here

# Rate Limiting
REQUESTS_PER_HOUR=1800
CONCURRENT_REQUESTS=5
RETRY_ATTEMPTS=5

# Debug Mode
DEBUG_MODE=false

# Apify Configuration
APIFY_TOKEN=your_apify_token_here
